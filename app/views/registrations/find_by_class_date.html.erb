<h1>Report</h1>
<h2><%= @class_date %> </h2>
<table class="table table-hover table-striped table-bordered">
  <tr>
    <th>Registration Date</th>
    <th>Orientation Date</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>StudentID</th>
    <th>Program</th>
    <th>Attended</th>
    <th></th>
    <th></th>
  </tr>

<% @registrations.each do |registration| %>
  <tr>
    <td><%= registration.created_at.to_date %></td>
		<% if registration.orientation != nil %>
    	<td><%= registration.orientation.class_date.to_date %></td>
		<% else %>
    	<td>Online</td>
		<% end %>
    <td><%= registration.first_name %></td>
    <td><%= registration.last_name %></td>
    <td><%= registration.email %></td>
    <td><%= registration.phone %></td>
    <td><%= registration.student_id %></td>
    <td><%= registration.program %></td>
		<% if registration.orientation != nil %>
    	<td><%= registration.checked_in %></td>
		<% else %>
    	<td>Online</td>
		<% end %>

  </tr>
<% end %>
</table>
<br />
